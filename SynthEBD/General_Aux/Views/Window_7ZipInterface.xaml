<Window x:Class="SynthEBD.Window_7ZipInterface"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SynthEBD"
        mc:Ignorable="d"
        Title="7Zip Mirror" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding PutThisOnScreen, Mode=OneWay}" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" IsReadOnly="True" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" Foreground="White" BorderBrush="White" BorderThickness="1" />
    </Grid>
</Window>
